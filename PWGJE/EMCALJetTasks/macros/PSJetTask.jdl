Jobtag = {
   "comment:Automatically generated analysis JDL"
};
# Input xml collections
InputDataCollection = {
   "LF:/alice/cern.ch/user/p/psteffan/PSJetTask/$1,nodownload"
};
# Output directory
OutputDir = "/alice/cern.ch/user/p/psteffan//PSJetTask/out/$2/#alien_counter_03i#";
# List of requested packagesList of requested packages
Packages = {
   "VO_ALICE@AliRoot::v5-09-09-1",
   "VO_ALICE@AliPhysics::vAN-20170628-1",
   "VO_ALICE@ROOT::v5-34-30-alice7-11",
   "VO_ALICE@APISCONFIG::V1.1x"
};
# List of input files to be uploaded to workers
InputFile = {
   "LF:/alice/cern.ch/user/p/psteffan/PSJetTask/PSJetTask.C",
   "LF:/alice/cern.ch/user/p/psteffan/PSJetTask/PSJetTask.root",
   "LF:/alice/cern.ch/user/p/psteffan/PSJetTask/AliAnalysisTaskPatJet.h",
   "LF:/alice/cern.ch/user/p/psteffan/PSJetTask/AliAnalysisTaskPatJet.cxx"
};
# This is the startup script
Executable = "/alice/cern.ch/user/p/psteffan/PSJetTask/PSJetTask.sh";
# We split per SE or file
Split = "se";
